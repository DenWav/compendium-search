{
  name: 'compendium-search',
  version: '1.0.0',
  description: 'A fast and lightweight compendium browser for FoundryVTT',
  main: 'dist/index.js',
  type: 'module',
  author: 'DenWav',
  license: 'AGPL-3.0-only',
  keywords: [
    'foundry',
    'foundry-vtt',
  ],
  scripts: {
    clean: 'gulp clean',
    build: 'gulp build',
    update: 'pnpm run build && cpy --cwd=./dist "./**" ../foundryData/Data/modules/compendium-search/',
    fix: 'gts fix',
  },
  repository: {
    type: 'git',
    url: 'https://github.com/DenWav/compendium-search',
  },
  bugs: {
    url: 'https://github.com/DenWav/compendium-search/issues',
  },
  dependencies: {
    flexsearch: '0.7.43',
  },
  devDependencies: {
    // branch v12/main
    '@league-of-foundry-developers/foundry-vtt-types': 'github:League-of-Foundry-Developers/foundry-vtt-types#ba7a9ca',
    gts: '5.3.1',
    gulp: '^5.0.0',
    'gulp-sourcemaps': '^3.0.0',
    'gulp-typescript': '6.0.0-alpha.1',
    rimraf: '4.4.1',
    typescript: '5.5.3',
  },
  // Fix some deprecation warnings
  pnpm: {
    overrides: {
      '@humanwhocodes/config-array': 'npm:@eslint/config-array@0.17.0',
      '@humanwhocodes/object-schema': 'npm:@eslint/object-schema@2.1.4',
      glob: '9.3.5',
      rimraf: '4.4.1',
      '@typescript-eslint/eslint-plugin': '7.15.0',
      '@typescript-eslint/parser': '7.15.0',
      css: 'npm:@adobe/css-tools@4.4.0',
    },
  },
}
